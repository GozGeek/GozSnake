0 REM SAVE"D:SPEED.BAS":LIST"D:SPEED.LST"
1 REM TESTING SPEED 2022-10-04
10 DIM A(100),B(100),A$(100),B$(100)
20 FOR T=0 TO 99
30 V=INT(RND(0)*39+1)
40 A(T)=V:B(T)=V
45 S=T+1
50 A$(S,S)=CHR$(V):B$(S,S)=CHR$(V)
100 NEXT T
110 PRINT "SHUFFLING ARRAY"
120 ARRSTART=(PEEK(18)*65536+PEEK(19)*256+PEEK(20))
130 FOR T=1 TO 99
140 A(T-1)=A(T):B(T-1)=B(T)
150 NEXT T
160 ARREND=(PEEK(18)*65536+PEEK(19)*256+PEEK(20))
200 PRINT ARREND-ARRSTART
300 ARRSTART=(PEEK(18)*65536+PEEK(19)*256+PEEK(20))
302 GOTO 340
305 FOR T=2 TO 100
310 A$(T-1,T-1)=A$(T,T)
320 B$(T-1,T-1)=B$(T,T)
330 NEXT T
340 A$=A$(2):B$=B$(2)
350 ARREND=(PEEK(18)*65536+PEEK(19)*256+PEEK(20))
400 PRINT ARREND-ARRSTART
